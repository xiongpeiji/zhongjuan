/* pages/Feedbacksuccess/Feedbacksuccess.wxss */
.send_love{
  padding: 30rpx 24rpx 30rpx 30rpx;
  margin-bottom: 26rpx;
  background: #fff;
}
.stutas_img{
  width: 100rpx;
  height: 100rpx;
  margin: auto;
}
.stutas_text{
  text-align: center;
  font-size: 40rpx;
  line-height: 56rpx;
  color:#E4572E;
  margin-bottom: 10rpx;
}
.tips{
  text-align: center;
  line-height: 34rpx;
  font-size: 24rpx;
  color:#A6A6A6;
}
.success_text .stutas_text{
  color:#333333;
}

.success_btnbox{
  width: 570rpx;
  margin: auto;
  margin-top: 80rpx;
  overflow: hidden;
}
.success_btnbox button{
  width: 212rpx;
  height: 74rpx;
  border-radius: 37rpx;
  line-height: 72rpx;
  font-size: 34rpx;
}

.success_btnbox button::after{
  border: none;
}
.success_btnbox .no_btn{
  float: left;
  border:1px solid #E4572E;
  color: #E4572E;
}
.success_btnbox .go_look{
  border:1px solid #E4572E;
  float: right;
  background: #E4572E;
  color: #fff;
}
