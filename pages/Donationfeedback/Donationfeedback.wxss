/* pages/Donationfeedback/Donationfeedback.wxss */
.juanzhu{
  padding: 26rpx 26rpx 30rpx 30rpx;
  background: #fff;
  margin-bottom: 26rpx;
  box-sizing: border-box;
  position: relative;
}
.jz_discript{
  padding-left: 248rpx;
  position: relative;
  height: 174rpx;
}
.jz_discript .img_tit{
  width: 230rpx;
  height: 174rpx;
  position: absolute;
  left: 0;
  top: 0;
}
.title_text{
  width:448rpx;
  height:90rpx;
  font-size:32rpx;
  color:#030303;
  line-height:45rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 12rpx;
}
.jg_name{
  font-size:24rpx;
  color:#030303;
  line-height:33rpx;
  margin-bottom: 8rpx;
}
.jg_type{
  float: left;
  height:33rpx;
  border-radius:17rpx;
  border:2rpx solid #E65E34;
  padding:0 15rpx;
  line-height: 31rpx;
  box-sizing: border-box;
  font-size: 0;
}
.jg_type .type_icon{
  width:17rpx;
  height:20rpx;
  float: left;
  margin-top: 5rpx;
  margin-right: 7rpx;
}
.jg_type label{
  font-size: 20rpx;
  color:#E65E34;
}
.jz_stutas{
  height:50rpx;
  background:#D82A3C;
  box-shadow:1px 2px 6px 0px rgba(251,212,216,1);
  border-top-left-radius: 25rpx;
  border-bottom-left-radius: 25rpx;
  display: inline-block;
  font-size: 24rpx;
  color:#fff;
  line-height: 50rpx;
  padding-left: 26rpx;
  padding-right: 26rpx;
  position: absolute;
  right: 0;
  bottom: 30rpx;
}
.jz_stutas image{
  /* float: right; */
  display: inline-block;
  width: 11rpx;
  height: 18rpx;
  margin-left: 14rpx;
}


.new_jilu{
  display: block;
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  overflow: hidden;
  font-size: 36rpx;
  color:#333333;
  font-weight: bold;
  margin-bottom: 6rpx;
}
.new_tit{
  float: left;
}
.new_jilu label{
  font-weight: normal;
  color:#969696;
  margin-left: 15rpx;
}
.look_more_btn{
  float: right;
  font-size: 24rpx;
  color:#B1B1B1;
  line-height: 50rpx;
}
.look_more_btn image{
  float: right;
  width: 18rpx;
  height: 30rpx;
  margin-top: 8rpx;
  margin-left: 10rpx;
}


.done_cont{
  padding:35rpx 30rpx 20rpx;
  box-sizing: border-box;
  background: #fff;
  margin-bottom: 36rpx;
}
.user_cont{
  padding-left: 94rpx;
  padding-bottom: 18rpx;
  position: relative;
  height: 80rpx;
  border-bottom: 1px solid #DADADA;
}

.user_cont .user_photo{
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  left: 0;
  top: 0;
  border-radius: 50%;
}

.name_time{
  float: left;
  font-size: 0;
}
.name_time view{
  font-size: 26rpx;
  color:#483C38;
  line-height: 36rpx;
  margin-top: 10rpx;
}
.name_time label{
  font-size: 22rpx;
  color:#999999;
}
.user_cont .look_more_btn{
  margin-top: 20rpx;
}
.wp_content{
  padding-top: 20rpx;
  overflow: hidden;
}
.wp_list{
  width: 50%;
  text-align: center;
  float: left;
  margin-bottom: 20rpx;
  font-size: 32rpx;
  color:#4D4D4D;
  line-height: 44rpx;
}

.no_messages {
  padding:100rpx;
}
.no_messages image{
  display: block;
  width: 192rpx;
  height: 104rpx;
  margin:auto;
  margin-bottom: 40rpx;
}
.no_messages .no_msg_text{
  text-align: center;
  font-size: 26rpx;
  color: #747474;
  margin-bottom: 50rpx;
}


.look_more_btn .isSuccess{
  color: #E65E34;
}
.the_end{
  background:#E0E0E0;
  box-shadow: none;
}